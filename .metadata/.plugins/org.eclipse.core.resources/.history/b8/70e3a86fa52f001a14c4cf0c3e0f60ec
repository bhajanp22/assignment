package com.unicommerce;

public class TV_Remove implements Remote_Contol {
	public static void main(String[] args) {

		Remote_Contol rmt = new TV_Remove();
		public int channel;
		rmt.start();
		rmt.stop();
		rmt.decreaseChannel();
		rmt.decreaseVolume();
		rmt.increaseChannel();
		rmt.increaseVolume();
		rmt.listAllChannel();
		rmt.setFavourite();
		rmt.mute();

	}

	@Override
	public void start() {
		// TODO Auto-generated method stub
		System.out.println("start");

	}

	@Override
	public void stop() {
		// TODO Auto-generated method stub
		System.out.println("Inside Stop");
	}

	@Override
	public void setChannel(int newchannel) {
		// TODO Auto-generated method stub
		channel=newchannel;
		System.out.println("Inside New Channel");
	}

	@Override
	public void increaseChannel() {
		// TODO Auto-generated method stub
		System.out.println("Inside Increase Channel");

	}

	@Override
	public void mute() {
		// TODO Auto-generated method stub
		System.out.println("Mute");

	}

	@Override
	public void setFavourite() {
		// TODO Auto-generated method stub
		System.out.println("Set Favourite");
	}

	@Override
	public void increaseVolume() {
		// TODO Auto-generated method stub
		System.out.println("Increase Volume");
	}

	@Override
	public void decreaseVolume() {
		// TODO Auto-generated method stub
		System.out.println("decrease Volume");

	}

	@Override
	public void decreaseChannel() {
		// TODO Auto-generated method stub
		System.out.println("decrease Channel");

	}

	@Override
	public void listAllChannel() {
		// TODO Auto-generated method stub
		System.out.println("List All Channel");

	}

}